{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"rootDir": "./",
		"baseUrl": "./",
		"outDir": "../dist",
		"tsBuildInfoFile": "../dist/.tsbuildinfo",
		"composite": true,
		"paths": {
			"#root/*": ["*"],
			"#lib/*": ["lib/*"]
		}
	},
	"include": [".", "**/*.json"],
	"exclude": ["tsconfig.json"]
}
